{
  "name": "ilopx/web-demos",
  "description": "Demonstration of web projects in the study of web technologies",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "ilopX",
      "email": "ilopX@ukr.net"
    }
  ],
  "autoload":{
    "psr-4":{
      "ilopx\\libs\\": "libs/"
    }
  },
  "autoload-dev":{
    "psr-4":{
      "ilopx\\demos\\": "demos",
      "ilopx\\demos\\console\\": "demos/console/",
      "ilopx\\demos\\web\\": "demos/web/"
    }
  },

  "require-dev": {
    "bower-asset/bootstrap": "^3",
    "michelf/php-markdown": "^1.5",
    "bower-asset/html5shiv": "*",
    "bower-asset/ace-builds": "^1.2",
    "bower-asset/ilopx-jquery-detect-change": "^0.8"
  },
  "extra": {
    "asset-installer-paths": {
      "bower-asset-library": "vendor/bower"
    }
  }
}
